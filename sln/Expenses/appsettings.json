{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=HouseHoldExpenses.db"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [ "Console" ]
  },
  "AllowedHosts": "*",
  "FileSizeLimit":  "200"
}
